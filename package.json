{
  "name": "angularjs-starter-bootstrap4",
  "version": "1.0.0",
  "description": "A Starter Project for AngularJS & Bootstrap4",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "clean-quick": "rimraf ./dist",
    "install-locked": "npm ci",
    "prebuild-local": "npm run clean-quick",
    "build-local": "cross-env NODE_ENV=local webpack",
    "prebuild-dev": "npm run clean-quick",
    "build-dev": "cross-env NODE_ENV=development webpack",
    "prebuild-prod": "npm run clean && npm ci",
    "build-prod": "cross-env NODE_ENV=production webpack",
    "start": "npm run build-local && webpack-dev-server --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "adam.caldwell",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@uirouter/angularjs": "^1.0.15",
    "angular": "^1.6.9",
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "@uirouter/visualizer": "^6.0.0",
    "angular-animate": "^1.6.9",
    "angular-mocks": "^1.6.9",
    "angular-touch": "^1.6.9",
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^1.1.10",
    "html-webpack-plugin": "webpack-contrib/html-webpack-plugin",
    "imports-loader": "^0.8.0",
    "ng-annotate-loader": "^0.6.1",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.7.2",
    "npm-check-updates": "^2.14.0",
    "path": "^0.12.7",
    "postcss-loader": "^2.1.1",
    "precss": "^3.1.1",
    "raw-loader": "^1.0.0-beta.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.0.1",
    "webpack-archive-plugin": "^3.0.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-cli": "^2.0.9",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^3.0.0",
    "webpack-merge": "^4.1.2"
  }
}
